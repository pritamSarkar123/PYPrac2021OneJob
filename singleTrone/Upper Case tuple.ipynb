    "class UpperCaseTuple(tuple):\n",
    "    def __new__(cls,iterable):\n",
    "        upper_iterable = (s.upper() for s in iterable)\n",
    "        return super().__new__(cls,upper_iterable)\n",
    "    \n",
    "print(UpperCaseTuple([\"pritam\",\"sarkar\"]))"
   ]
